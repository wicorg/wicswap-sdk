{
  "name": "@wicswap/pcsx-sdk",
  "version": "13.0.0",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "dependencies": {
    "@wicswap/chains": "workspace:*",
    "@wicswap/swap-sdk-core": "workspace:*",
    "@wicswap/permit2-sdk": "workspace:*",
    "dayjs": "^1.11.10",
    "viem": "^2.22.23"
  },
  "devDependencies": {
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
