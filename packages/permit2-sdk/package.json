{
  "name": "@wicswap/permit2-sdk",
  "version": "13.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "An sdk for interacting with permit2.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wicorg/wicswap-sdk.git",
    "directory": "packages/permit2-sdk"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\""
  },
  "keywords": [
    "monswap",
    "ethereum"
  ],
  "dependencies": {
    "@wicswap/chains": "workspace:*",
    "@wicswap/sdk": "workspace:*",
    "tiny-invariant": "^1.3.1",
    "viem": "^2.22.23"
  },
  "devDependencies": {
    "tslib": "^2.6.2",
    "tsup": "^6.7.0"
  }
}
