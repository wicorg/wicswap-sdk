{
  "name": "@wicswap/routing-sdk-addon-stable-swap",
  "version": "11.0.0",
  "description": "Routing sdk addon for pcs stable swap",
  "repository": {
    "type": "git",
    "url": "https://github.com/wicorg/wicswap-sdk.git",
    "directory": "packages/routing-sdk/addons/stable-swap"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "test:u": "vitest --run -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typechecks": "tsc --noEmit -p ./tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@wicswap/routing-sdk": "workspace:*"
  },
  "dependencies": {
    "@wicswap/chains": "workspace:*",
    "@wicswap/stable-swap-sdk": "workspace:*",
    "@wicswap/swap-sdk-core": "workspace:*",
    "tiny-invariant": "^1.3.0"
  },
  "devDependencies": {
    "@wicswap/routing-sdk": "workspace:*",
    "@wicswap/tsconfig": "workspace:*",
    "@wicswap/utils": "workspace:*",
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
