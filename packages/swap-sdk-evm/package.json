{
  "name": "@wicswap/swap-sdk-evm",
  "version": "11.1.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "An SDK for Monswap EVM based chains",
  "repository": {
    "type": "git",
    "url": "https://github.com/MonchainDev/wicswap-sdk.git",
    "directory": "packages/swap-sdk-evm"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "test:u": "vitest --run -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typechecks": "tsc --noEmit -p ./tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@wicswap/chains": "workspace:*",
    "@wicswap/swap-sdk-core": "workspace:*",
    "tiny-invariant": "^1.3.0",
    "tiny-warning": "^1.0.3",
    "viem": "^2.22.23"
  },
  "devDependencies": {
    "@wicswap/tsconfig": "workspace:*",
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
